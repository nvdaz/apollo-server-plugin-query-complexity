{
  "name": "apollo-server-plugin-query-complexity",
  "version": "4.0.0",
  "type": "module",
  "description": "Apollo server plugin that limits query complexity.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "nvdaz/apollo-server-plugin-query-complexity"
  },
  "files": [
    "src",
    "dist"
  ],
  "peerDependencies": {
    "@apollo/server": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-query-complexity": "^0.12.0"
  },
  "devDependencies": {
    "@apollo/server": "^4.0.0",
    "@types/jest": "^29.2.2",
    "graphql": "^16.6.0",
    "graphql-query-complexity": "^0.12.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build"
  }
}
